<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ymsStyle">
        <item name="android:textColor">@color/textoColor1</item>
        <item name="android:textSize">35dp</item>
    </style>
    <style name="ymsStyleLabel">
        <item name="android:textColor">@color/textoColor1 </item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">35dp </item>
        <item name="android:layout_gravity">center|bottom</item>
    </style>
    <style name="ymsStyleButton">
        <item name="android:textColor">@color/textoColor1 </item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14dp </item>
        <item name="backgroundTint">@color/fondoColor3</item>
        <item name="android:textAllCaps">false </item>
    </style>
    <style name="ymsStyleEditText">
        <item name="android:textColor">@color/textoColor1 </item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20dp </item>
        <item name="backgroundTint">@color/fondoColor1</item>
    </style>
    <style name="ymsStyleTitulo">
        <item name="android:textColor">@color/textoTitulo </item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">28dp </item>
        <item name="android:textStyle">bold </item>

    </style>
    <style name="ymsStyleSubtitulo">
        <item name="android:textColor">@color/textoTitulo </item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">25dp </item>
        <item name="android:textStyle">bold </item>
    </style>
    <style name="ymsStyleServicios">
        <item name="android:textColor">@color/black </item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">19dp </item>
        <item name="android:textStyle">bold </item>

    </style>
    <style name="ymsStyleSubervicios">
        <item name="android:textColor">@color/black </item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12dp </item>
        <item name="android:textStyle">bold </item>

    </style>
</resources>